<div class="container">
  <div class="row">
    <div class="col-md-12 center">
      <h1>Accounts</h1>
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <% if current_user %>
        <p> You are logged in as <h2><%= link_to current_user[:name], user_path(current_user) %></h2> </p>
      <% end %>
        <p>  <%= link_to "log-out", log_out_path %></p>
        <p><%= link_to "Register New Account", new_user_path %>
        </p>
        <p>
          <%= link_to "Login to an Existing Account", log_in_path %>
        </p>
      <% end %>
      <% end %>
    </div>
  </div>
